The code in this package was autogenerated by the fiat-crypto project
at version v0.0.9 from a formally verified model.

    docker build -t fiat-crypto:v0.0.9 .
    go install github.com/mmcloughlin/addchain/cmd/addchain@v0.3.1-0.20211027081849-6a7d3decbe08
    docker run --rm --entrypoint unsaturated_solinas fiat-crypto:v0.0.9 \
        --lang Go --no-wide-int --cmovznz-by-mul \
        --internal-static --public-function-case camelCase --public-type-case camelCase \
        --private-function-case camelCase --private-type-case camelCase \
        --no-prefix-fiat --package-name fiat --doc-text-before-function-name '' \
        --doc-prepend-header 'Code generated by Fiat Cryptography. DO NOT EDIT.' \
        --doc-newline-before-package-declaration p521 64 9 '2^521 - 1' \
        carry_mul carry_square carry add sub to_bytes from_bytes selectznz \
        > p521_fiat64.go && ../../../../../bin/go fmt p521_fiat64.go
    addchain gen -tmpl invert.tmpl <(addchain search '2^521 - 3') | \
        sed 's/Element/P521Element/g' | ../../../../../bin/gofmt > p521_invert.go
    docker run --rm --entrypoint word_by_word_montgomery fiat-crypto:v0.0.9 \
        --lang Go --no-wide-int --relax-primitive-carry-to-bitwidth 32,64 --cmovznz-by-mul \
        --internal-static --public-function-case camelCase --public-type-case camelCase \
        --private-function-case camelCase --private-type-case camelCase \
        --no-prefix-fiat --package-name fiat --doc-text-before-function-name '' \
        --doc-prepend-header 'Code generated by Fiat Cryptography. DO NOT EDIT.' \
        --doc-newline-before-package-declaration p384 64 '2^384 - 2^128 - 2^96 + 2^32 - 1' \
        mul square add sub one from_montgomery to_montgomery selectznz to_bytes from_bytes \
        > p384_fiat64.go && ../../../../../bin/go fmt p384_fiat64.go
    addchain gen -tmpl invert.tmpl <(addchain search '2^384 - 2^128 - 2^96 + 2^32 - 3') | \
        sed 's/Element/P384Element/g' | ../../../../../bin/gofmt > p384_invert.go
    docker run --rm --entrypoint word_by_word_montgomery fiat-crypto:v0.0.9 \
        --lang Go --no-wide-int --relax-primitive-carry-to-bitwidth 32,64 --cmovznz-by-mul \
        --internal-static --public-function-case camelCase --public-type-case camelCase \
        --private-function-case camelCase --private-type-case camelCase \
        --no-prefix-fiat --package-name fiat --doc-text-before-function-name '' \
        --doc-prepend-header 'Code generated by Fiat Cryptography. DO NOT EDIT.' \
        --doc-newline-before-package-declaration p224 64 '2^224 - 2^96 + 1' \
        mul square add sub one from_montgomery to_montgomery selectznz to_bytes from_bytes \
        > p224_fiat64.go && ../../../../../bin/go fmt p224_fiat64.go
    addchain gen -tmpl invert.tmpl <(addchain search '2^224 - 2^96 - 1') | \
        sed 's/Element/P224Element/g' | ../../../../../bin/gofmt > p224_invert.go

